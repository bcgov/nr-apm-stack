{
  "index_patterns": [
    "nrm-accessrollup-*"
  ],
  "template": {
    "aliases": {
      "nrm-accessrollup": {}
    },
    "settings": {
      "number_of_shards": 1,
      "number_of_replicas": 1
    },
    "mappings": {
      "properties": {
        "@timestamp": {
          "properties": {
            "date_histogram": {
              "type": "date"
            }
          }
        },
        "url_path_keyword": {
          "properties": {
            "terms": {
              "type": "keyword"
            }
          }
        },
        "url_domain": {
          "properties": {
            "terms": {
              "type": "keyword"
            }
          }
        },
        "event_outcome_count": {
          "type": "long"
        },
        "event_duration_avg": {
          "type": "double"
        }
      }
    }
  },
  "version": 2,
  "priority": 300,
  "_meta": {
    "description": "NRM Access Url Rollup indices"
  }
}
