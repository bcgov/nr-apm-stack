{
  "name": "workflow-cli",
  "description": "OpenSearch Workflow CLI",
  "version": "1.0.0",
  "author": "Matthew Bystedt",
  "bin": {
    "workflow-cli": "./bin/run"
  },
  "dependencies": {
    "@aws-crypto/sha256-js": "^5.2.0",
    "@aws-sdk/client-elasticsearch-service": "^3.987.0",
    "@aws-sdk/client-sqs": "^3.987.0",
    "@aws-sdk/client-sts": "^3.987.0",
    "@aws-sdk/credential-provider-node": "^3.972.6",
    "@oclif/core": "^4.8.0",
    "@oclif/plugin-help": "^6.2.36",
    "@oclif/plugin-plugins": "^5.4.54",
    "@smithy/node-http-handler": "^4.4.5",
    "@smithy/protocol-http": "^5.3.5",
    "@smithy/signature-v4": "^5.3.5",
    "@types/ejs": "^3.1.3",
    "axios": "^1.13.2",
    "ejs": "^3.1.10",
    "hpagent": "^1.2.0",
    "inversify": "^6.2.2",
    "ora": "^5.4.1",
    "reflect-metadata": "^0.2.2",
    "tar-stream": "^3.1.6"
  },
  "devDependencies": {
    "@oclif/test": "^4.1.15",
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.10.1",
    "@types/tar-stream": "^3.1.4",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "chai": "^6.2.1",
    "eslint": "^9.39.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "mocha": "^10.8.2",
    "npm-pack-zip": "^1.3.0",
    "oclif": "^4.22.47",
    "shx": "^0.4.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0"
  },
  "dirname": "oex",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/configuration-opensearch",
    "/configuration-reindex",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "workflow-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint \"src/**/*.{js,ts}\" --quiet --fix",
    "check-lint": "eslint \"src/**/*.{js,ts}\" --quiet",
    "pack": "npm-pack-zip",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "manifest": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
